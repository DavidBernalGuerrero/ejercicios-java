 */

  public class DibujoSimbolos {			
    public static void main(String[] args) {

      System.out.println("    ");
      System.out.println("    ______        \\/  ");
      System.out.println("   /      \\           ");
      System.out.println("  /        \\          ");
      System.out.println("  \\        /          ");
      System.out.println("   \\_    _/           ");
      System.out.println("     |  |             ");
      System.out.println("     |  |   _______   ");
      System.out.println(" ____|  |___|_'-'_|___");

    }
  }
